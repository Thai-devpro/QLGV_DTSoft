@model QLGV_DTSoft.Data.KeHoachCongViec

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Thêm mới</h1>

<h4>kế hoạch công việc</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Namthuchien" class="control-label">Năm thực hiện</label>
                <input asp-for="Namthuchien" class="form-control" />
                <span asp-validation-for="Namthuchien" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Noidungcongviec" class="control-label">Nội dung công việc</label>
                <input asp-for="Noidungcongviec" class="form-control" />
                <span asp-validation-for="Noidungcongviec" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Thêm" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Về danh sách</a>
</div>

